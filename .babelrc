{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "ie >= 10"]
      }
    }],
    "react",
    "stage-0"
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    }
  },
  "plugins": [
    "syntax-dynamic-import",
    "transform-decorators-legacy",
    "transform-runtime",
    ["typecheck", {
      "disable": {
        "production": true
      }
    }
    ]
  ]
}
